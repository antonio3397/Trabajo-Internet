CREATE TABLE USUARIO (ID BIGINT NOT NULL, LOCALIDAD VARCHAR(255), NIF VARCHAR(255), APELLIDOS VARCHAR(255), CODIGO_POSTAL INTEGER, CONTRASENIA VARCHAR(255), CUOTA_TOTAL INTEGER, DIRECCION VARCHAR(255), EMAIL VARCHAR(255), FECHA_BAJA DATE, FECHA_INGRESO DATE, FECHA_NACIMIENTO DATE, METODO_PAGO VARCHAR(255), MOVIL INTEGER, NOMBRE VARCHAR(255), PROVINCIA VARCHAR(255), RESPONSABLE_LEGAL VARCHAR(255), SEXO VARCHAR(255), TELEFONO INTEGER, PRIMARY KEY (ID))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(15), PRIMARY KEY (SEQ_NAME))
